{
  "manifest_version": 2,
  "name": "IITG Captcha Solver",
  "version": "1.0",
  "description": "An extension to automatically solve the annoying Captcha",
  "permissions": [
    "activeTab",
    "http://localhost:5000/",
    "webRequest",    
    "storage",
    "<all_urls>"],

  "browser_action": {
    "default_popup": "popup.html"
  }
}
